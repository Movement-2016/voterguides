<!DOCTYPE html>
<html>
  <head>
    <title>Voterguides</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <p>
        <%= current_user.name %>
        <%= link_to "Log out", logout_path %>
      </p>
    <% end %>
    <%= yield %>
  </body>
</html>
